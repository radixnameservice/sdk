{
  "name": "@radixnameservice/rns-sdk",
  "version": "3.7.4",
  "description": "The official toolkit for the Radix Name Service",
  "type": "module",
  "scripts": {
    "test": "jest",
    "dev": "nodemon",
    "start": "node --no-warnings --loader ts-node/esm --experimental-specifier-resolution=node ./src/index.ts",
    "build": "tsup"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "dependencies": {
    "@radixdlt/babylon-gateway-api-sdk": "^1.10.1",
    "bech32": "^2.0.0",
    "decimal.js": "^10.5.0"
  },
  "devDependencies": {
    "@radixdlt/radix-dapp-toolkit": "^2.2.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.14",
    "ts-jest": "^29.2.6",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  }
}
